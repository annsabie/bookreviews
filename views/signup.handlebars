<div class = "card" id="signup-box">
<div class="card-header">
<h2>Sign Up</h2>
</div>
<div id="signup" role="tabpanel">
<form id="sign-up-form" class="card-body">
<div>
<label for="firstname-input-signup">First-Name</label>
<input type="text" id="firstname-input-signup"/>
</div>
<div>
<label for="lastname-input-signup">Last-Name</label>
<input type="text" id="lastname-input-signup"/>
</div>
<div>
<label for="email-input-signup">Email</label>
<input type="text" id="email-input-signup"/>
</div>
<div>
<label for="password-input-signup">Password</label>
<input type="text" id="password-input-signup"/>
</div>
<button type="submit" id="signup-btn" class="btn">Sign up</button>
</form>
<div id="loginHereLink">
Already have an account? Login <a href="/login">here</a>
</div>
</div>
</div>

<script src="/js/signup.js"></script>
